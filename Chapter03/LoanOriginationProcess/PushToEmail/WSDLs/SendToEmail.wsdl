<wsdl:definitions name="SendToEmail"
                  targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/ums/LoanOriginationProcess/PushToEmail/SendToEmail"
                  xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
                  xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/ums/LoanOriginationProcess/PushToEmail/SendToEmail"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:imp1="http://platform.integration.oracle/blocks/adapter/fw/metadata/SendToEmail"
                  xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/">
  <plt:partnerLinkType name="SendNotification_plt">
    <plt:role name="SendNotification_role">
      <plt:portType name="tns:SendNotification_ptt"/>
    </plt:role>
  </plt:partnerLinkType>
  <wsdl:types>
    <schema xmlns="http://www.w3.org/2001/XMLSchema">
      <import namespace="http://platform.integration.oracle/blocks/adapter/fw/metadata/SendToEmail"
              schemaLocation="../Schemas/SendToEmail.xsd"/>
    </schema>
  </wsdl:types>
  <wsdl:message name="SendNotification_msg">
    <wsdl:part name="body" element="imp1:message"/>
  </wsdl:message>
  <wsdl:portType name="SendNotification_ptt">
    <wsdl:operation name="SendNotification">
      <wsdl:input message="tns:SendNotification_msg"/>
    </wsdl:operation>
  </wsdl:portType>
</wsdl:definitions>
